{
  "name": "rerror",
  "version": "1.1.1",
  "description": "rich error",
  "main": "./rerror",
  "scripts": {
    "test": "npm run lint ; nyc mocha",
    "lint": "eslint .",
    "mocha": "mocha -R nyan",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "coveralls": "nyc report --reporter text-lcov | coveralls"
  },
  "repository": "borisdiakur/rerror",
  "keywords": [
    "error",
    "rich",
    "exception",
    "verror",
    "cause"
  ],
  "author": "Boris Diakur <contact@borisdiakur.com> (https://github.com/borisdiakur)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "1.2.0",
    "nyc": "^15.0.0",
    "open-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
